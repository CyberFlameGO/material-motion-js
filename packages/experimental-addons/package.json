{
  "name": "material-motion-experimental-addons",
  "version": "0.0.0",
  "description": "Experimental additions to the Material Motion API.  Anything here may eventually be migrated to the core, to its own package, or deleted entirely.  Use at your own risk.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "typescript:main": "src/index.ts",
  "scripts": {
    "lint": "$( yarn bin )/tslint -c ../../tslint.json --project tsconfig.json",
    "test": "node ../../tools/test.js --only experimental-addons",
    "clean": "rm -rf ./dist/*; mkdir -p ./dist/",
    "build": "yarn run clean; $( yarn bin )/tsc"
  },
  "dependencies": {
    "indefinite-observable": "0.3.0",
    "material-motion-streams": "0.0.0",
    "tslib": "^1.2.0"
  },
  "devDependencies": {
    "tslint": "4.0.2",
    "typescript": "2.1"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com:material-motion/material-motion-js.git"
  },
  "keywords": [
    "animation",
    "gestures",
    "interactive",
    "material",
    "motion",
    "multitouch",
    "observables",
    "odeon",
    "physics",
    "springs",
    "touch",
    "transitions",
    "tweens"
  ],
  "author": "The Material Motion Authors (see AUTHORS)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/material-motion/material-motion-js/issues/"
